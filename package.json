    "concurrently": "^6.2.0",
    "live-server": "^1.2.1",
    "node-sass": "^6.0.1",
    "postcss": "^8.3.5",
    "postcss-import": "^14.0.2",
    "prettier": "^2.3.2",
    "rollup": "^2.53.1",
    "rollup-plugin-delete": "^2.0.0",
